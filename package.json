{
  "name": "svelte-kit-app",
  "version": "0.0.15",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "clean": "rm -rf dist out node_modules"
  },
  "devDependencies": {
    "@rgossiaux/svelte-headlessui": "1.x",
    "@sveltejs/adapter-auto": "1.x",
    "@sveltejs/adapter-cloudflare": "^2.0.0",
    "@sveltejs/kit": "1.x",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-svelte3": "4.x",
    "prettier": "2.x",
    "prettier-plugin-svelte": "2.x",
    "svelte": "3.x",
    "svelte-check": "3.x",
    "svelte-preprocess": "5.x",
    "tslib": "2.x",
    "typescript": "4.x",
    "vite": "4.x"
  },
  "type": "module",
  "dependencies": {
    "@picocss/pico": "1.x",
    "@rollup/plugin-commonjs": "24.x",
    "@rollup/plugin-json": "6.x",
    "@rollup/plugin-node-resolve": "15.x",
    "bun-types": "0.5.x",
    "rollup": "3.x",
    "svelte-adapter-bun": "0.4.x"
  }
}
